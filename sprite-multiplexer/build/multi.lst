                        	    44: sp3y:           .equ vic + $07
                        	    45: sp4x:           .equ vic + $08
                        	    46: sp4y:           .equ vic + $09
                        	    47: sp5x:           .equ vic + $0a
                        	    48: sp5y:           .equ vic + $0b
                        	    49: sp6x:           .equ vic + $0c
                        	    50: sp6y:           .equ vic + $0d
                        	    51: sp7x:           .equ vic + $0e
                        	    52: sp7y:           .equ vic + $0f
                        	    53: msigx:          .equ vic + $10
                        	    54: scroly:         .equ vic + $11
                        	    55: raster:         .equ vic + $12
                        	    56: spena:          .equ vic + $15
                        	    57: vicirq:         .equ vic + $19
                        	    58: irqmsk:         .equ vic + $1a
                        	    59: extcol:         .equ vic + $20
                        	    60: sp0col:         .equ vic + $27
                        	    61: sp1col:         .equ vic + $28
                        	    62: sp2col:         .equ vic + $29
